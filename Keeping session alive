val sparkExcelReadConfiguration = Map(
  "dataAddress" -> "A1", // Explicitly define the starting cell
  "header" -> "true",
  "treatEmptyValuesAsNulls" -> "false",
  "inferSchema" -> "false",
  "useHeader" -> "true",
  "excerptSize" -> "0",
  "maxRowsInMemory" -> "20",
  "sheetName" -> "Sheet1" // Exact match with case sensitivity
)

val df = spark.read
  .format("com.crealytics.spark.excel")
  .options(sparkExcelReadConfiguration)
  .load("/data/gcganamfrmu/raw/169912/excel/Book1.xlsx")
