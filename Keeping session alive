def bubble_sort(arr):
    """
    Sorts the input list using the bubble sort algorithm.
    
    :param arr: List of elements to be sorted
    :type arr: list
    """
    n = len(arr)
    for i in range(n):
        swapped = False
        for j in range(0, n-i-1):
            if arr[j] > arr[j+1]:
                swapped = True
                arr[j], arr[j+1] = arr[j+1], arr[j]

        # If no swaps happened, the list is already sorted.
        if not swapped:
            break

    return arr  # Optional, as the input list is modified in place

if __name__ == '__main__':
    arr = [64, 34, 25, 12, 22, 11, 90]
    sorted_arr = bubble_sort(arr)
    print("Sorted array is:", ' '.join(map(str, sorted_arr)))
